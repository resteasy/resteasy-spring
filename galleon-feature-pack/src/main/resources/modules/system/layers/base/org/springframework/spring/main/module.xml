<module xmlns="urn:jboss:module:1.9" name="org.springframework.spring">
    <resources>
        <artifact name="${org.springframework:spring-aop}"/>
        <artifact name="${org.springframework:spring-beans}"/>
        <artifact name="${org.springframework:spring-context}"/>
        <artifact name="${org.springframework:spring-context-support}"/>
        <artifact name="${org.springframework:spring-core}"/>
        <artifact name="${org.springframework:spring-expression}"/>
        <artifact name="${org.springframework:spring-web}"/>
        <artifact name="${org.springframework:spring-webmvc}"/>
        <!-- MicroMeter is a dependency of spring-web -->
        <artifact name="${io.micrometer:micrometer-observation}"/>
        <artifact name="${io.micrometer:micrometer-commons}"/>
        <artifact name="${aopalliance:aopalliance}"/>
        <artifact name="${org.aspectj:aspectjrt}"/>
        <artifact name="${org.aspectj:aspectjweaver}"/>
    </resources>
    <dependencies>
        <!-- for java.beans -->
        <module name="java.desktop"/>
        <module name="java.logging"/>
        <module name="ch.qos.cal10n" optional="true"/>
        <module name="com.google.guava"/>
        <module name="jakarta.annotation.api"/>
        <module name="jakarta.el.api"/>
        <module name="jakarta.enterprise.api"/>
        <module name="jakarta.ejb.api"/>
        <module name="jakarta.faces.api"/>
        <module name="jakarta.interceptor.api"/>
        <module name="jakarta.persistence.api"/>
        <module name="jakarta.servlet.api"/>
        <module name="jakarta.servlet.jsp.api"/>
        <module name="jakarta.transaction.api"/>
        <module name="org.jboss.jandex"/>
        <module name="org.jboss.staxmapper"/>
        <module name="org.jboss.as.controller"/>
        <module name="org.jboss.as.ee"/>
        <module name="org.jboss.as.ejb3"/>
        <module name="jakarta.xml.bind.api"/>
        <module name="org.apache.commons.logging" export="true"/>
        <module name="org.jboss.as.jpa"/>
        <module name="org.jboss.as.naming"/>
        <module name="org.jboss.as.server"/>
        <module name="org.jboss.as.transactions"/>
        <module name="org.jboss.logging"/>
        <module name="org.jboss.modules"/>
        <module name="org.jboss.msc"/>
        <module name="org.jboss.vfs"/>
        <module name="org.slf4j"/>
        <module name="org.slf4j.ext" optional="true"/>
    </dependencies>
</module>


